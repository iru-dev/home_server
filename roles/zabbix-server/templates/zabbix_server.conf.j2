ListenIP=0.0.0.0
CacheSize=1G
StartDBSyncers=30
HistoryCacheSize=2G
TrendCacheSize=2G
ValueCacheSize=2G
Timeout=15
UnreachablePeriod=60
UnavailableDelay=30
UnreachableDelay=30
AlertScriptsPath=/etc/zabbix/alertscripts
ExternalScripts=/etc/zabbix/externalscripts
FpingLocation=/usr/sbin/fping
Fping6Location=/usr/sbin/fping6
HistoryCacheSize = 2G
HistoryIndexCacheSize = 2G
